<!DOCTYPE html>
<html lang="en">
    <% include ../partials/head %>
    <body>
        <% include ../partials/header %>
        <main class="page-content">
            <section class="content">
                <section class="description">
                    <% include ../partials/user/user-global-toolbar %>
                    <% include ../partials/error-box %>
                    <section>
                        <ul>
                            <% for (let [id, user] of Object.entries(users)) { %>
                                <li>
                                    <a href='/user/<%= user.id %>'><%= user.login %></a>
                                </li>
                            <% } %>
                        </ul>
                    </section>
                    <% include ../partials/user/users-pages  %>
                    <% include ../partials/user/users-toolbar %>
                </section>
            </section>
        </main>
        <% include ../partials/footer %>
    </body>
</html>

